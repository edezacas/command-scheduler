{
    "name": "edc/command-scheduler",
    "version": "1.0.0",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "description": "Symfony Command Scheduler",
    "authors": [
        {
            "name": "Eduard Deza",
            "email": "edezacas@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4.0",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/orm": "^2.8",
        "symfony/config": "^5.2",
        "symfony/dependency-injection": "^4.0|^5.0",
        "symfony/http-kernel": "^4.0|^5.0"
    },
    "require-dev": {
        "symfony/debug": "^4.4",
        "symfony/test-pack": "^1.0",
        "symfony/yaml": "^5.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "EDC\\CommandSchedulerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EDC\\CommandSchedulerBundle\\Tests\\": "tests/"
        }
    }
}
